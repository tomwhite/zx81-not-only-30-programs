9990 LET L = 100
9991 LET N = 16509
9992 IF PEEK N > 38 THEN STOP
9993 POKE N, INT (L/256)
9994 POKE N+1, L - 256 * INT (L/256)
9995 FOR N = N + 4 TO N + 3 + PEEK(N+2) + \
       256 * PEEK (N+3)
9996 IF PEEK N = 236 OR PEEK N = 237 THEN \
       PRINT L; "-"; CHR$ PEEK N
9997 NEXT N
9998 LET L = L + 10
9999 GOTO 9992